<?xml version='1.0'?>

<schema xmlns='http://www.w3.org/2001/XMLSchema' targetNamespace='Assembly' xmlns:a='Assembly'>
  
  <!-- normal elements -->
  <element name='if' type='a:any' />
  <element name='unless' type='a:any' />
  <element name='dowhile' type='a:any' />
  <element name='nop' type='a:empty' />
  <element name='tag'>
    <complexType>
      <attribute name='name' type='ID' use='required' />
    </complexType>
  </element>
  <element name='goto'>
    <complexType>
      <attribute name='cond' type='a:coditionType' use='optional' />
      <attribute name='target' type='IDREF' use='required' />
    </complexType>
  </element>
  <element name='call'>
    <complexType>
      <attribute name='target' type='IDREF' use='required' />
    </complexType>
  </element>
  <element name='skip'>
    <complexType>
      <sequence>
        <group ref='a:anyElement' minOccurs='0' maxOccurs='unbounded' />
      </sequence>
      <attribute name='cond' type='a:coditionType' use='optional' />
    </complexType>
  </element>
  <element name='contractcall'>
    <complexType>
      <attribute name='flag' type='a:callFlagType' use='optional' default='All' />
      <attribute name='method' type='Name' use='required' />
      <attribute name='hash' type='a:hash160Literal' use='required'></attribute>
    </complexType>
  </element>
  <element name='syscall'>
    <complexType>
      <attribute name='name' type='a:syscallType' use='required' />
    </complexType>
  </element>
  <element name='instruction'>
    <complexType>
      <attribute name='operand' type='a:oprandType' use='optional' />
      <attribute name='opcode' type='a:opcodeType' use='required' />
    </complexType>
  </element>

  <!-- the two most important elements: lazy and func-->
  <element name='lazy'>
    <complexType>
      <sequence>
        <element ref='a:meta' minOccurs='0' maxOccurs='1' />
        <element ref='a:std' minOccurs='0' maxOccurs='unbounded' />
        <element ref='a:event' minOccurs='0' maxOccurs='unbounded' />
        <group ref='a:anyElement' minOccurs='0' maxOccurs='unbounded' />
      </sequence>
      <attribute name='bytecode' type='string' use='optional' />
      <attribute name='result' type='string' use='optional' />
    </complexType>
  </element>
  <element name='func'>
    <complexType>
      <sequence>
        <element ref='a:arg' minOccurs='0' maxOccurs='unbounded' />
        <group ref='a:anyElement' minOccurs='0' maxOccurs='unbounded' />
      </sequence>
      <attribute name='name' type='Name' use='required' />
      <attribute name='safe' type='a:boolLiteral' use='optional' />
      <attribute name='return' type='a:literalType' use='optional' />
    </complexType>
  </element>

  <!-- contract definition related elements -->
  <element name='meta'>
    <complexType>
      <simpleContent>
        <extension base='string'>
          <attribute name='compiler' type='a:MaxLen64String' use='optional' default='neoml' />
          <attribute name='src' type='string' use='optional' default='neo.org' />
          <attribute name='name' type='Name' use='required' />
        </extension>
      </simpleContent>
    </complexType>
  </element>
  <element name='std'>
    <complexType>
      <annotation>
        <documentation>
          Supported Standard: NEP-17 NEP-11 ...
        </documentation>
      </annotation>
      <attribute name='std' type='a:stdType' use='required' />
    </complexType>
  </element>
  <element name='event'>
    <complexType>
      <sequence minOccurs='0' maxOccurs='unbounded'>
        <element ref='a:arg' />
      </sequence>
      <attribute name='name' type='Name' use='required' />
    </complexType>
  </element>
  <element name='arg'>
    <complexType>
      <attribute name='name' type='Name' use='required' />
      <attribute name='type' type='a:literalType' use='required' />
    </complexType>
  </element>

  <!-- literal elements -->
  <element name='compound'>
    <complexType>
      <sequence>
        <element ref='a:lazy' minOccurs='0' maxOccurs='unbounded' />
        <group ref='a:literal' minOccurs='0' maxOccurs='unbounded' />
        <element ref='a:compound' minOccurs='0' maxOccurs='unbounded' />
      </sequence>
      <attribute name='type' type='a:literalCompoundType' use='optional' default='array' />
      <attribute name='size' type='integer' use='optional' />
    </complexType>
  </element>
  <element name='int'>
    <complexType>
      <attribute name='val' type='a:intLiteral' use='required' />
    </complexType>
  </element>
  <element name='bool'>
    <complexType>
      <attribute name='val' type='a:boolLiteral' use='optional' default='false' />
    </complexType>
  </element>
  <element name='string'>
    <complexType>
      <attribute name='val' type='string' use='required' />
    </complexType>
  </element>
  <element name='bytes'>
    <complexType>
      <attribute name='val' type='a:bytesLiteral' use='required' />
    </complexType>
  </element>
  <element name='null' type='a:empty' />
  <element name='hash160'>
    <complexType>
      <attribute name='val' type='a:hash160Literal' use='required' />
    </complexType>
  </element>
  <element name='hash256'>
    <complexType>
      <attribute name='val' type='a:hash256Literal' use='required' />
    </complexType>
  </element>
  <!-- <element name='address'>
    <complexType>
      <attribute name='val' type='a:addressLiteral' use='required' />
    </complexType>
  </element>
  <element name='publickey'>
    <complexType>
      <attribute name='val' type='a:publickeyLiteral' use='required' />
    </complexType>
  </element> -->

  <!-- specific types -->
  <complexType name='empty' />
  <complexType name='any'>
    <sequence minOccurs='0' maxOccurs='unbounded'>
      <group ref='a:anyElement' />
    </sequence>
  </complexType>
  <simpleType name='MaxLen64String'>
    <restriction base='string'>
      <maxLength value='64' />
    </restriction>
  </simpleType>
  <simpleType name='stdType'>
    <union memberTypes='a:wellknownstd string' />
  </simpleType>
  <simpleType name='oprandType'>
    <restriction base='string'>
      <pattern value='([0-9a-fA-F][0-9a-fA-F])*' />
    </restriction>
  </simpleType>

  <!-- literal types -->
  <simpleType name='intLiteral'>
    <restriction base='string'>
      <pattern value='-?[0-9]{1,78}' />
    </restriction>
  </simpleType>
  <simpleType name='bytesLiteral'>
    <restriction base='string'>
      <pattern value='([0-9a-fA-F][0-9a-fA-F])*' />
    </restriction>
  </simpleType>
  <simpleType name='boolLiteral'>
    <restriction base='string'>
      <enumeration value='true' />
      <enumeration value='false' />
    </restriction>
  </simpleType>
  <simpleType name='hash256Literal'>
    <restriction base='string'>
      <pattern value='0x[0-9a-fA-F]{64}' />
    </restriction>
  </simpleType>
  <!-- <simpleType name='addressLiteral'>
    <restriction base='string'>
      <pattern value='N[1-9A-HJ-NP-Za-km-z]{33}' />
    </restriction>
  </simpleType>
  <simpleType name='publickeyLiteral'>
    <restriction base='string'>
      <pattern value='[0-9a-fA-F]{66}' />
    </restriction>
  </simpleType> -->
  <simpleType name='basichash160'>
    <restriction base='string'>
      <pattern value='0x[0-9a-fA-F]{40}' />
    </restriction>
  </simpleType>
  <simpleType name='hash160Literal'>
    <annotation>
      <documentation>ContractManagement</documentation>
      <documentation>0xfffdc93764dbaddd97c48f252a53ea4643faa3fd</documentation>
      <documentation>CryptoLib</documentation>
      <documentation>0x726cb6e0cd8628a1350a611384688911ab75f51b</documentation>
      <documentation>GAS</documentation>
      <documentation>0xd2a4cff31913016155e38e474a2c06d08be276cf</documentation>
      <documentation>Ledger</documentation>
      <documentation>0xda65b600f7124ce6c79950c1772a36403104f2be</documentation>
      <documentation>NEO</documentation>
      <documentation>0xef4073a0f2b305a38ec4050e4d3d28bc40ea63f5</documentation>
      <documentation>Oracle</documentation>
      <documentation>0xfe924b7cfe89ddd271abaf7210a80a7e11178758</documentation>
      <documentation>Policy</documentation>
