      <documentation>StdLib</documentation>
      <documentation>0xacce6fd80d44e1796aa0c2c625e9e4e0ce39efc0</documentation>
      <documentation>NeoBurger</documentation>
      <documentation>0x48c40d4666f93408be1bef038b6722404d9a4c2a</documentation>
      <documentation>NoBug</documentation>
      <documentation>0x54806765d451e2b0425072730d527d05fbfa9817</documentation>
    </annotation>
    <union memberTypes='a:wellknowncontracts a:basichash160' />
  </simpleType>

  <!-- groups -->
  <!-- literal group -->
  <!-- any element group -->
  <group name='literal'>
    <choice>
      <element ref='a:int' />
      <element ref='a:bool' />
      <element ref='a:string' />
      <element ref='a:bytes' />
      <element ref='a:null' />
      <element ref='a:hash160' />
      <element ref='a:hash256' />
      <!-- <element ref='a:address' />
      <element ref='a:publickey' /> -->
    </choice>
  </group>
  <group name='anyElement'>
    <choice>
      <group ref='a:literal' />
      <element ref='a:lazy' />
      <element ref='a:nop' />
      <element ref='a:tag' />
